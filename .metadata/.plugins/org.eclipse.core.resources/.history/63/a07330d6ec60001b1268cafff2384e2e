class Countdown{
	
	public void docountdown() {
		String color;
		
		switch (Thread.currentThread().getName()) {
		case "thread 1":
			color = ThreadColor.ANSI_C;
			break;
		case "thread 2":
			color = ThreadColor.ANSI_p;
			break;
		default:
			break;
		}
		
	}
	
	
}